\providecommand{\ip}[1][]{\partial\ifthenelse{\equal{#1}{}}{}{\left( #1 \right)}}
\providecommand{\idd}[2]{\frac{\,\mathrm{d} #1}{\,\mathrm{d} #2}}
\providecommand{\ipp}[2]{\frac{\partial #1}{\partial #2}}

